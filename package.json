{
  "name": "encrypted-todo",
  "version": "1.0.0",
  "main": "electron/main.js",
  "scripts": {
    "start": "concurrently \"npm run start-main\" \"npm run start-react\"",
    "start-main": "tsc -p . && electron .",
    "start-react": "cd src && parcel index.html --open",
    "build": "tsc -p ."
  },
  "dependencies": {
    "electron": "^26.1.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "sqlcipher": "^5.0.0",
    "tailwindcss": "^3.3.0"
  },
  "devDependencies": {
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@types/node": "^20.0.0",
    "typescript": "^5.0.0",
    "postcss": "^8.0.0",
    "autoprefixer": "^10.0.0",
    "concurrently": "^7.0.0",
    "parcel": "^2.0.0"
  }
}
